  if (debugInfo.initializationAttempts === 0) {
    debugInfo.domReadyState = domStateManager.getReadyState();
    tryInitialize();
  }
}).catch((error) => {
  logError('‚ùå DOM ready promise rejected:', error);
  // Fallback to immediate initialization
  tryInitialize();
});

